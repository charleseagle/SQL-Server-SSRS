  <DataSets>
    <DataSet Name="StudioCertificateRunTime">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <CommandText>SELECT        tblCertificate.Certificate, tblStudio.StudioName, tblFilm.FilmRunTimeMinutes
FROM            tblCertificate INNER JOIN
                         tblFilm ON tblCertificate.CertificateID = tblFilm.FilmCertificateID INNER JOIN
                         tblStudio ON tblFilm.FilmStudioID = tblStudio.StudioID
ORDER BY tblCertificate.Certificate</CommandText>
      </Query>
      <Fields>
        <Field Name="Certificate">
          <DataField>Certificate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StudioName">
          <DataField>StudioName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmRunTimeMinutes">
          <DataField>FilmRunTimeMinutes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
