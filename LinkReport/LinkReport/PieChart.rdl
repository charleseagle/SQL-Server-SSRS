  <DataSets>
    <DataSet Name="CountryFilms">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <CommandText>SELECT        tblCountry.CountryID, tblCountry.CountryName, tblFilm.FilmBoxOfficeDollars
FROM            tblCountry INNER JOIN
                         tblFilm ON tblCountry.CountryID = tblFilm.FilmCountryID
ORDER BY tblCountry.CountryName</CommandText>
      </Query>
      <Fields>
        <Field Name="CountryID">
          <DataField>CountryID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CountryName">
          <DataField>CountryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmBoxOfficeDollars">
          <DataField>FilmBoxOfficeDollars</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
