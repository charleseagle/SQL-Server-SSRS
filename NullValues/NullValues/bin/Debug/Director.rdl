  <DataSources>
    <DataSource Name="Director">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=eagle;Initial Catalog=Movies</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>aea8d0a9-fcb6-484a-847f-475d7f0534f4</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Director">
      <Query>
        <DataSourceName>Director</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@prmDirector">
            <Value>=Parameters!prmDirector.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        tblFilm.FilmName, tblFilm.FilmReleaseDate, tblDirector.DirectorName
FROM            tblFilm INNER JOIN
                         tblDirector ON tblFilm.FilmDirectorID = tblDirector.DirectorID
WHERE	(DirectorID = @prmDirector OR @prmDirector IS NULL)
ORDER BY tblDirector.DirectorName</CommandText>
      </Query>
      <Fields>
        <Field Name="FilmName">
          <DataField>FilmName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmReleaseDate">
          <DataField>FilmReleaseDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="DirectorName">
          <DataField>DirectorName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Directors">
      <Query>
        <DataSourceName>Director</DataSourceName>
        <CommandText>SELECT       DirectorID, DirectorName, DirectorName AS SortField
FROM            tblDirector
UNION
SELECT NULL, 'Show all directors', '0'
ORDER BY SortField</CommandText>
      </Query>
      <Fields>
        <Field Name="DirectorID">
          <DataField>DirectorID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DirectorName">
          <DataField>DirectorName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SortField">
          <DataField>SortField</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
