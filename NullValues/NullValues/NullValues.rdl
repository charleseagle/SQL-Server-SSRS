  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FilmName">
            <Value>=Parameters!FilmName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MeanLength">
            <Value>=Parameters!MeanLength.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        FilmName, FilmReleaseDate, FilmRunTimeMinutes
FROM            tblFilm
WHERE        (FilmName = @FilmName OR
                         @FilmName IS NULL) 
	AND (FilmRunTimeMinutes &gt;= @MeanLength OR
                         @MeanLength IS NULL)
	AND (FilmReleaseDate &gt; = @StartDate Or @StartDate  IS NULL)
ORDER BY FilmName</CommandText>
      </Query>
      <Fields>
        <Field Name="FilmName">
          <DataField>FilmName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmReleaseDate">
          <DataField>FilmReleaseDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="FilmRunTimeMinutes">
          <DataField>FilmRunTimeMinutes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <CommandText>SELECT        DirectorName
FROM            tblDirector
ORDER BY DirectorName</CommandText>
      </Query>
      <Fields>
        <Field Name="DirectorName">
          <DataField>DirectorName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
