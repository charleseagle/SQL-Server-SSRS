  <DataSets>
    <DataSet Name="OscarWins">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <CommandText>SELECT        tblFilm.FilmName, tblFilm.FilmOscarWins, tblCountry.CountryName
FROM            tblFilm INNER JOIN
                         tblCountry ON tblFilm.FilmCountryID = tblCountry.CountryID
ORDER BY tblFilm.FilmName DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="FilmName">
          <DataField>FilmName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
