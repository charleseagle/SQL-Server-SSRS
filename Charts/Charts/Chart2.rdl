  </DataSources>
  <DataSets>
    <DataSet Name="FilmStudioCertificate">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <CommandText>SELECT        tblCertificate.Certificate, tblStudio.StudioName, tblFilm.FilmName
FROM            tblCertificate INNER JOIN
                         tblFilm ON tblCertificate.CertificateID = tblFilm.FilmCertificateID INNER JOIN
                         tblStudio ON tblFilm.FilmStudioID = tblStudio.StudioID
ORDER BY tblFilm.FilmName DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="Certificate">
          <DataField>Certificate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StudioName">
          <DataField>StudioName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmName">
          <DataField>FilmName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
