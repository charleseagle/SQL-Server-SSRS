    <DataSet Name="dtsDirectors">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <CommandText>SELECT        DirectorName, DirectorID
FROM            tblDirector
ORDER BY DirectorName</CommandText>
      </Query>
      <Fields>
        <Field Name="DirectorName">
          <DataField>DirectorName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DirectorID">
