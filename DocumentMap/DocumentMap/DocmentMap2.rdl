  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <CommandText>SELECT        tblFilm.FilmName, tblFilm.FilmReleaseDate, tblFilm.FilmBudgetDollars, tblStudio.StudioName
FROM            tblFilm INNER JOIN
                         tblStudio ON tblFilm.FilmStudioID = tblStudio.StudioID
ORDER BY tblFilm.FilmName</CommandText>
      </Query>
